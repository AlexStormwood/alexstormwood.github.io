{
  "name": "alex-stormwood-journal",
  "version": "5.0.2",
  "license": "MIT",
  "description": "Online journal of Alex Stormwood, based on a high-performance 11ty blog template.",
  "scripts": {
    "build-ci": "npm run js-build && eleventy && npm run test",
    "build": "npm run clean && npm run build-ci",
    "watch": "concurrently \"npm run serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "npm run clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "rm -Rf ./_site/posts/*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AlexStormwood/alexstormwood.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/AlexStormwood/alexstormwood.github.io/issues"
  },
  "homepage": "https://github.com/AlexStormwood/alexstormwood.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^16.5.4",
    "fs-extra": "^8.1.0",
    "jsdom": "^16.5.0",
    "luxon": "^2.5.2",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^5.2.5",
    "node-fetch": "^2.6.7",
    "rollup-plugin-terser": "^6.1.0",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@ampproject/toolbox-optimizer": "^2.9.0",
    "@kevingimbel/eleventy-plugin-mermaid": "^2.1.0",
    "any-shell-escape": "^0.1.1",
    "clean-css": "^4.2.3",
    "concurrently": "^7.0.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "expect.js": "^0.3.1",
    "ffmpeg-static": "^4.4.0",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lru-cache": "^5.1.1",
    "markdown-it-image-figures": "^2.1.1",
    "mocha": "^10.1.0",
    "phin": "^3.5.0",
    "photoswipe": "^5.4.4",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.3",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sharp": "^0.30.5",
    "touch": "^3.1.0",
    "twitter": "^1.7.1"
  }
}
